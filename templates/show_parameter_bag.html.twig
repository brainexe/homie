{% if parameters  %}
	<h1>{{ title }}</h1>
	<table class="table table-bordered table-striped table-hover">
		<thead>
			<tr>
				<th>Key</th>
				<th>Value</th>
			</tr>
		</thead>
		<tbody>
			{% for key, value in parameters %}
				<tr>
					<td>{{ key }}</td>
					<td>{{ value }}</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endif %}