{
  "name": "brainexe/raspberry",
  "description": "HomeControl via RaspberryPi",
  "homepage": "https://mdoetsch.de",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Matze",
      "email": "matze@mdoetsch.de"
    }
  ],
  "autoload": {
    "psr-0": {
      "Raspberry": "src/"
    }
  },
  "require": {
    "brainexe/core": "dev-master",
    "brainexe/template": "dev-master",
    "brainexe/assets": "dev-master",
    "brainexe/totp": "dev-master",
    "brainexe/input-control": "dev-master",
    "brainexe/search": "dev-master",
    "league/flysystem": "~1.1@dev",
    "league/flysystem-webdav": "~1.0@dev",
    "league/flysystem-cached-adapter": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.5.*"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "suggest": {
    "ext-phpiredis": "Allows faster serialization and deserialization of the Redis protocol"
  }
}
