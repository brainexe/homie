{
  "ImageProcessor": [],
  "TemplateProcessor": [],
  "JsProcessor": [
    {
      "name": "sensor.js",
      "files": [
        "^bower_components\/rickshaw\/vendor\/d3.v3.js",
        "^bower_components\/rickshaw\/rickshaw.js"
      ]
    },
    {
      "name": "mood.js",
      "files": [
        "^bower_components\/EaselJS\/lib\/easeljs-0.8.0.min.js",
        "^js\/controllers\/mood\/.*"
      ]
    },
    {
      "name": "vendor.js",
      "files": [
        "^bower_components\/jquery\/dist/jquery.min.js",
        "^bower_components\/sockjs-client\/dist\/sockjs.js",
        "^bower_components\/angular\/angular.min.js",
        "^bower_components\/angular-route\/angular-route.min.js",
        "^bower_components\/angular-gettext\/dist\/angular-gettext.min.js",
        "^bower_components\/requirejs\/require.js",
        "^js\/vendor\/.*.js"
      ]
    },
    {
      "name": "common.js",
      "files": [
        "^js\/app.js",
        "^js\/util\/.*",
        "^js\/controllers\/.*",
        "^lang\/.*js"
      ]
    }
  ],
  "CssProcessor": [
    {
      "name": "common.css",
      "files": [
        "^bower_components\/bootstrap\/dist\/css\/bootstrap.min.css",
        "^bower_components\/rickshaw\/rickshaw.css",
        "^css\/.*.css"
      ]
    }
  ],
  "CopyFilesProcessor": [
    {
	  "name": "fonts\/",
      "files": ["bower_components/bootstrap/fonts/glyphicons-*"]
    }
  ]
}
