<?xml version="1.0" encoding="utf-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="logger.sensor.log">logs/sensor.log</parameter>
		<parameter key="application.name">raspberry</parameter>
		<parameter key="application.registration_enabled">false</parameter>

		<parameter key="raspberry_client.class"></parameter>

		<parameter key="delete_sensor_values" type="collection">
			<parameter type="collection">
				<parameter key="days">7</parameter>
				<parameter key="percentage">25</parameter>
			</parameter>
			<parameter type="collection">
				<parameter key="days">30</parameter>
				<parameter key="percentage">50</parameter>
			</parameter>
		</parameter>
		<parameter key="assets.priorities" type="collection">
			<parameter>js/jquery-2.1.0.min.js</parameter>
			<parameter>rickshaw/d3.min.js</parameter>
			<parameter>rickshaw/d3.layout.min.js</parameter>
			<parameter>js/eventemitter2.js</parameter>
			<parameter>js/moment.min.js</parameter>
			<parameter>js/createjs.min.js</parameter>
			<parameter>js/script.js</parameter>
		</parameter>
    </parameters>

	<services>
		<service id="RaspberryClient" alias="RaspberryClient.Local"/>
	</services>
</container>
